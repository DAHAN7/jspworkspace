<!-- js_bom/02_bom_console.html -->
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
	// 출력 탭
	window.console.log("기본");
	
	let a = "hello";
	let b = 3.141592;
	let c = false;
	
	console.log(a + b + c);
	console.log(a , b , c);
	
	// pattern
	console.log("%s는 문자열, %d는 수치형", a , b);
	console.log("%s는 문자열, %f는 실수", a , b);
	console.info("정보는 log랑 동일하게 출력");
	console.warn("경고!!!!!");
	console.error("오류!!!!!!!!");
	
	// console 출력된 내용을 청소
	console.clear();
	
	// 디버깅 시에 연산에 소비된 시간을 체크하는 함수
	console.time();
</script>
</head>
<body>

</body>
</html>




