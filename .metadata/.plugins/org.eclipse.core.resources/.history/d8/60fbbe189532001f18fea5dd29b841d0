<script>
	// 출력 탭
	window.console.log("기본");
	
	let a = "hello";
	let b = 3.141592;
	let c = false;
	
	console.log(a + b + c);
	console.log(a , b , c);
	
	// pattern
	console.log("%s는 문자열, %d는 수치형", a , b);
	console.log("%s는 문자열, %f는 실수", a , b);
	
	
</script>
